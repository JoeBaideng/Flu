                <action name="多通阀切换" device_id="4" value="3"></action>
                <action name="设置流速泵" device_id="1" value="1"></action>
                <action name="泵启动" device_id="3" address="1" value="1"></action>
                <action name="泵关闭" device_id="3" address="1" value="0"></action>
            </program>
            <program id="cleaning" name="清理">
