                <device id="5" name="电磁阀b" slave_id="201" address="12"/>
            </group>
        </device_groups>
        <programs>
            <program id="loading_sample" name="上样">
                <action name="电磁阀断电" device_id="2" value="0"></action>
                <action name="设置流速泵" device_id="1" value="0"></action>
                <action name="泵启动" device_id="3" address="1" value="1"></action>
                <action name="泵关闭" device_id="3" address="1" value="0"></action>
            </program>
            <program id="washing" name="淋洗">
                <action name="电磁阀通电" device_id="2" value="1"></action>
                <action name="多通阀切换" device_id="4" value="2"></action>
                <action name="设置流速泵" device_id="1" value="1"></action>
